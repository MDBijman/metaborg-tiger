module examples

control-flow rules

For(binding, body) = 
    entry -> binding -> body -> exit,
                        body -> binding

VarDec(n, t, e) = entry -> e -> this -> exit

root FunDec(n, args, rt, body) = start -> body -> end

properties

live: MaySet[string]

property rules

live(_.end) = {}
live(VarDec(n, _, _) -> next) = {m | m <- live(next), m != n}
live(Var(n) -> next) = {n} \/ live(next)  
live(_ -> next) = live(next)
